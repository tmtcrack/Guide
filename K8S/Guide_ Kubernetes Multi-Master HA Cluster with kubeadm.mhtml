  <span class=3D"token property">"CN"</span><span class=3D"token operator">=
:</span> <span class=3D"token string">"Kubernetes"</span><span class=3D"tok=
en punctuation">,</span>
  <span class=3D"token property">"key"</span><span class=3D"token operator"=
