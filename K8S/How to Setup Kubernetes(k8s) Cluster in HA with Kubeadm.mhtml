#---------------------------------------------------------------------
frontend apiserver
&nbsp;&nbsp;&nbsp; bind *:8443
&nbsp;&nbsp;&nbsp; mode tcp
&nbsp;&nbsp;&nbsp; option tcplog
&nbsp;&nbsp;&nbsp; default_backend apiserver
#---------------------------------------------------------------------
# round robin balancing for apiserver
#---------------------------------------------------------------------
backend apiserver
&nbsp;&nbsp;&nbsp; option httpchk GET /healthz
&nbsp;&nbsp;&nbsp; http-check expect status 200
&nbsp;&nbsp;&nbsp; mode tcp
&nbsp;&nbsp;&nbsp; option ssl-hello-chk
&nbsp;&nbsp;&nbsp; balance&nbsp;&nbsp;&nbsp;&nbsp; roundrobin
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; server k8s-master-1 192.168.1.40=
:6443 check
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; server k8s-master-2 192.168.1.41=
:6443 check
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; server k8s-master-3 192.168.1.42=
:6443 check</pre>
